<block>
    <view class="list" bindtap="detail">
        <view class="list.title">{{title}}</view>
        <view class="list.content" style="{{anonymousState__temp}}">{{content}}</view>
        <view>
            <block>
                <block wx:if="{{content === '九点起床！'}}">
                    <view class="{{true}}" style="height:48px;line-height:48px;color:#ddd;margin-right:8px">></view>
                </block>
                <view style="width:14px;height:48px;line-height:48px" wx:else></view>
            </block>
        </view>
    </view>
</block>